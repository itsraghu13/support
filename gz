import gzip

file_path_A = "path_to_file_A.gz"
file_path_B = "path_to_file_B.gz"

def read_and_compare_files(file_path1, file_path2):
    matching_lines = []

    with gzip.open(file_path1, "rt") as file1, gzip.open(file_path2, "rt") as file2:
        lines1 = file1.readlines()
        lines2 = file2.readlines()

    # Normalize and compare the lines
    for line1, line2 in zip(lines1, lines2):
        line1 = line1.strip('"').strip()  # Remove double quotes and leading/trailing whitespace
        line2 = line2.strip('"').strip()
        
        if line1 == line2:
            matching_lines.append(line1)

    return matching_lines

matching_lines = read_and_compare_files(file_path_A, file_path_B)

if matching_lines:
    print("Matching lines found:")
    for line in matching_lines:
        print(line)
else:
    print("No matching lines found between the files.")
